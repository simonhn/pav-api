	width:700px;
}
	#number_overlay{
	  	  display: block;
	      position: absolute;
	      width: 100%;
	      top: 7%;
	      left: -5;
	      z-index: 2;
	      text-align: right;
	      font-size: 70px;
	      letter-spacing: -13px;
	      font-weight: bold;
				font-family:arial,helvetica,sans-serif;
	}
#front_page_playlist_section
{height:145px;}
